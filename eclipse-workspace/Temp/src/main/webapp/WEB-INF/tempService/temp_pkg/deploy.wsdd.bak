<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from tempService WSDL service -->

  <service name="temp" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://temp"/>
      <parameter name="wsdlServiceElement" value="tempService"/>
      <parameter name="schemaQualified" value="http://temp"/>
      <parameter name="wsdlServicePort" value="temp"/>
      <parameter name="className" value="temp_pkg.TempSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="temp"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="toFahrenheit" qname="operNS:toFahrenheit" xmlns:operNS="http://temp" returnQName="retNS:toFahrenheitReturn" xmlns:retNS="http://temp" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:temp" xmlns:pns="http://temp" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="toCelsius" qname="operNS:toCelsius" xmlns:operNS="http://temp" returnQName="retNS:toCelsiusReturn" xmlns:retNS="http://temp" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:temp" xmlns:pns="http://temp" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="toCelsius toFahrenheit"/>

  </service>
</deployment>
